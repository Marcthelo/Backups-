CREATE SCHEMA `rsp_banco` DEFAULT CHARACTER SET utf8;

CREATE TABLE `rsp_banco`.`produtos`(
  `codigo` INT NOT NULL AUTO_INCREMENT,
  `automovel` enum('Carro','Trator','Caminh√£o','Moto','Motoneta') NOT NULL,
  `tipo` varchar(255) not null,
  `class` varchar(255) not null,
  `volts` float(5,2) not null,
  `fab` varchar(255) not null,
  PRIMARY KEY(`codigo`))
  ENGINE = InnoDB
  DEFAULT CHARACTER SET = utf8;
