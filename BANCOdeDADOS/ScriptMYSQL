CREATE SCHEMA `rsp_banco` DEFAULT CHARACTER SET utf8;

CREATE TABLE `rsp_banco`.`produtos_induzido`(
  `codigo` INT NOT NULL AUTO_INCREMENT,
  `descricao` VARCHAR(255) NOT NULL,
  `fabricante` VARCHAR(255) NOT NULL,
/*Informações Técnicas*/`info_tec` VARCHAR(255) NOT NULL,
  `preco` VARCHAR(255) NOT NULL,
  PRIMARY KEY(`codigo`))
  ENGINE = InnoDB
  DEFAULT CHARACTER SET = utf8;
